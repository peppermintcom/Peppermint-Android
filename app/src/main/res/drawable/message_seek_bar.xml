<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="3dp" android:gravity="center_vertical">
        <shape>
            <solid android:color="#d6e1dd" />
            <corners android:radius="2dp" />
            <size android:height="3dp" />
        </shape>
    </item>
    <item android:id="@android:id/progress" android:height="3dp" android:gravity="center_vertical">
        <clip>
            <shape>
                <solid android:color="#2bb09f" />
                <corners android:radius="2dp" />
                <size android:height="3dp" />
            </shape>
        </clip>
    </item>
</layer-list>