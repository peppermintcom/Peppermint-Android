    <item android:id="@android:id/background">
        <shape
            android:innerRadiusRatio="3"
            android:shape="ring"
            android:useLevel="false"
            android:angle="0"
            android:type="sweep"
            android:thicknessRatio="10.0">
            <solid android:color="#d1dcdb"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">

            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:angle="0"
                android:type="sweep"
                android:thicknessRatio="10.0">
                <solid android:color="#85a09d"/>
            </shape>

    </item>
</layer-list>